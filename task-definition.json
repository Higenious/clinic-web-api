{
  "family": "web-api-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::711538462588:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::711538462588:role/ecsTaskRoleSNSAccess",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "myclinic-app-container",
      "image": "711538462588.dkr.ecr.us-east-1.amazonaws.com/myclinic-api:PLACEHOLDER",
      "essential": true,
      "environment": [
        {
          "name": "MONGO_URI",
          "value": "__MONGO_URI__"
        },
        {
          "name": "TWILIO_ACCOUNT_SID",
          "value": "__TWILIO_ACCOUNT_SID__"
        },
        {
          "name": "TWILIO_AUTH_TOKEN",
          "value": "__TWILIO_AUTH_TOKEN__"
        },
        {
          "name": "TWILIO_WHATSAPP_NUMBER",
          "value": "__TWILIO_PHONE_NUMBER__"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/myclinic-api-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
